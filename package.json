{
    "version": "0.4.1",
    "name": "jikji",
    "scripts": {
        "sandbox": "pnpm --filter @jikji/sandbox start",
        "generator": "pnpm --filter @jikji/generator start",
        "docs": "pnpm --filter @jikji/website start --port 3001",
        "build": "pnpm run build-react & pnpm run build-generator",
        "build-react": "pnpm --filter @jikji/react build",
        "test-react": "pnpm --filter @jikji/react test",
        "build-docs": "pnpm --filter @jikji/website build",
        "test-generator": "pnpm --filter @jikji/generator test",
        "build-generator": "pnpm --filter @jikji/generator build",
        "example": "pnpm --filter example start",
        "example-ssg": "pnpm --filter example start-ssg"
    },
    "packageManager": "pnpm@10.18.3",
    "dependencies": {
        "eslint": "^8.33.0",
        "prettier": "^2.8.3",
        "typescript": "^4.9.4"
    },
    "devDependencies": {
        "@types/eslint": "^8.4.10",
        "@types/prettier": "^2.7.2"
    }
}
